{
    "Lua.runtime.version": "LuaJIT",
    "Lua.workspace.library": [
        "sm.lua",
    ],
    "Lua.diagnostics.disable": [
        "duplicate-set-field",
        "lowercase-global",
        "redundant-parameter",
        "need-check-nil",
        "undefined-field",
        "unbalanced-assignments",
        "undefined-global",
        "param-type-mismatch",
        "inject-field",
        "missing-parameter",
        "assign-type-mismatch",
        "redefined-local",
        "cast-local-type",
        "redundant-return-value"
    ],
    "files.associations": {
        "*.layout": "xml",
        "*.json": "jsonc",
        "*.tooldb": "jsonc",
        "*.toolset": "jsonc",
        "*.relayout": "jsonc"
    }
}